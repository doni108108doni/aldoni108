{
  "spec": {
    "title": "CSSOM View Module",
    "url": "https://drafts.csswg.org/cssom-view/"
  },
  "events": [
    {
      "href": "https://drafts.csswg.org/cssom-view/#eventdef-mediaquerylist-change",
      "src": {
        "format": "summary table",
        "href": "https://drafts.csswg.org/cssom-view/#eventdef-mediaquerylist-change"
      },
      "type": "change",
      "targets": [
        "MediaQueryList"
      ],
      "interface": "MediaQueryListEvent"
    },
    {
      "href": "https://drafts.csswg.org/cssom-view/#eventdef-window-resize",
      "src": {
        "format": "summary table",
        "href": "https://drafts.csswg.org/cssom-view/#eventdef-window-resize"
      },
      "type": "resize",
      "targets": [
        "Window",
        "VisualViewport"
      ],
      "interface": "Event"
    },
    {
      "href": "https://drafts.csswg.org/cssom-view/#eventdef-document-scroll",
      "src": {
        "format": "summary table",
        "href": "https://drafts.csswg.org/cssom-view/#eventdef-document-scroll"
      },
      "type": "scroll",
      "targets": [
        "Document",
        "Element",
        "VisualViewport"
      ],
      "interface": "Event",
      "bubbles": true
    },
    {
      "href": "https://drafts.csswg.org/cssom-view/#eventdef-document-scrollend",
      "src": {
        "format": "summary table",
        "href": "https://drafts.csswg.org/cssom-view/#eventdef-document-scrollend"
      },
      "type": "scrollend",
      "targets": [
        "Document",
        "Element"
      ],
      "interface": "Event",
      "bubbles": true
    }
  ]
}